SINADR:	JSR GT1FIX
	LDX #NARG1
	JSR CHKINT
	BCS SCALL2
	LDA NARG1+1
	BNE SINAD3	;>255 means it is an address for sure.
	CMP NARG1
	BNE SINAD2
	SETV NARG1,KEYIN	;0, reset to kbd driver
	JMP SOTAD3
SINAD2:	LDA NARG1
	CMP #$07	;1 to 7 means set to C#00.
	BCC SINAD3	;8-255, ok, if you really want to...
	LDA NARG1
	ORA #$C0
	STA NARG1+1
	LDA #$00
	STA NARG1
SOTAD3:	MOV DEFINP,INPDEV,NARG1	;default input and inpdev get narg1
	JMP POPJ
