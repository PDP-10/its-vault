;;; -*- Mode: LISP; Package: CADR -*-

(defun cc-dl11 ()
  (:init-dl11-unibus-channel)
  (setq dbg-use-dl11 t)
  (cc))

(defun dbg-repeat (loc &optional value)
  (cond (value (dbg-write loc value)
	       (format 'dl11-stream "~O;~O;" (lsh loc -1) value))
	(t (dbg-read loc)
	   (format 'dl11-stream "~O\" (lsh loc -1)))))
