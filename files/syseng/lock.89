ATTACH:	MOVEI A,1
	MOVEM A,GUNDET	;1 FOR ATTACH.
	JRST DETAC0

DETACH:	SETOM GUNDET	;-1 FOR DETACH.
	JRST DETAC0

GUN:	SETZM GUNDET	;0 FOR GUN.
DETAC0:	MOVEI A,SYMSU
	PUSHJ P,INITS1
	SKIPGE B,TNUM
	JRST QLUP
	JUMPE B,GUN1
	PUSHJ P,USER
	JRST QLUP
	JUMPE U,QLUP
	MOVEM U,USTORE
	MOVEM J,JSTORE
	PUSHJ P,TCRR
	MOVE C,GUNDET
	MOVE C,[[SIXBIT \DETACH !\]
		[SIXBIT \GUN DOWN !\]
		[SIXBIT \ATTACH !\]]+1(C)
	PUSHJ P,ITYOUT
	MOVEI C,USTORE
	PUSHJ P,ITYOUT
	MOVEI C,JSTORE
	PUSHJ P,ITYOUT
	.IOT TYOC,["?]
	PUSHJ P,QHACK1
	PUSHJ P,USER
	JRST QLUP
	CAMN U,USTORE
	CAME J,JSTORE
	JRST DETAC0
GUNACT:	SKIPE GUNDET
	 JRST DETAC1	;DETACH OR ATTACH.
	.GUN B,
	JRST QLUP
	JRST BLUP

DETAC1:	.CALL [SETZ? 'DETACH ? 1000,,400000(B)]
	JFCL
	SKIPG GUNDET
	 JRST BLUP	;DETACH.
	.CALL [SETZ? 'ATTACH ? 1000,,4^5(B)]
	 JRST QLUP
	.VALUE [ASCIZ/:LOGOUT /]	;ATTACH.

GUNDET:	0

USER:	MOVE C,B
	IMUL C,LUBLK
	HRLZ D,USRHI
	HRRI D,D
	.GETLOC D,
	AOS (P)
	CAMG D,C
	SOS (P)
	MOVE D,C
	ADD D,UNAME
	HRLI D,U
	MOVSS D
	.GETLOC D,
	ADD C,JNAME
	HRLI C,J
	MOVSS C
	.GETLOC C,
	POPJ P,

GUN1:	MOVEI B,[ASCIZ \
UNAME=\]
	PUSHJ P,QEXPL2
	PUSHJ P,6NAMEG
	MOVE F,[SIXBIT \HACTRN\]
	JUMPN C,GUN3
	MOVE F,E
	MOVEI B,[ASCIZ \
JNAME=\]
	PUSHJ P,QEXPL2
	PUSHJ P,6NAMEG
	EXCH E,F
GUN3:	MOVEI B,0
GUN2:	PUSHJ P,USER
	JRST QLUP
	CAME U,E
	AOJA B,GUN2
	CAME J,F
	AOJA B,GUN2
	JRST GUNACT

USTORE:	0
	SIXBIT \ !\
JSTORE:	0
	SIXBIT \!\

6NAMEG:	MOVE D,[440600,,E]
	MOVEI E,0
6NAME1:	.IOT TYIC,C
	SUBI C,40
	CAIL C,100
	JRST QLUP
	JUMPLE C,CPOPJ
	TLNE D,770000
	IDPB C,D
	JRST 6NAME1

NOTNUM:	CAIE A,177
	CAIN A,15
	JRST BLUP
NOTNM2:	SUBI A,40
	JUMPLE A,QLUP
	TLNE E,770000
	IDPB A,E
	MOVSI F,-CTABL
	CAME D,COMTAB(F)
	AOBJN F,.-1
	JUMPG F,CLUP
	HRRZ F,COMT2(F)
