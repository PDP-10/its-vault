	.SUPSET F,
	MOVEI C,[SIXBIT /SYS CHECKER !/]
	PUSHJ P,TITYOT
	MOVEI C,[SIXBIT /UN!/]
	TRNE F,1
	PUSHJ P,ITYOUT
