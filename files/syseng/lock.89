	ANDI B,77
	SKIPN B
	JRST CTEST2	;CH NOT OPEN
	CAIE B,61
	JRST CTEST	;CH PROBABLY BEING USED TO LOCK TTY
CDPB1:	.UCLOSE .,
CTEST2:	.GENSYM B,
	MOVEM B,XJNAME
