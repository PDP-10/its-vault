;-*-MIDAS-*-

	title PANDA randomness for AI

a=1
b=2
c=3
d=4
e=5
f=6
cnt=7
p=17

;;; IO channels
tyoch==1
linch==2
idxch==3
odxch==4

pdlsiz==10
.scalar pdl(pdlsiz)

define	syscall	name,args
	.call	[setz ? sixbit /name/ ? args((setz))]
	termin

.insrt syseng;byelib >

begin:	move	p,[-pdlsiz,,pdl-1]
	syscal	open,[%clbit,,.uao ? %climm,,tyoch ? [sixbit/tty/]]
	 .lose	%lsfil
	pushj	p,byelib"getmsg
	syscal	siot,[%climm,,tyoch ? %clin,,a ? %clin,,b]
	 .lose	%lssys
	syscal	finish,[%climm,,tyoch]	;wait for all output to get to tty.
	 .lose	%lssys
	.logout 1,

end begin
