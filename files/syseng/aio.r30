TYOB:	PUSH P,B
	CAIN A,^Q
	JRST TYOB3	;^Q => READ FROM TAPE
	CAIN A,^S
	JRST TYOB5	;^S => TO READ CHECK PTR THEN TTY (NO RUBOUT)
	MOVEI B,PIPUN
	CAIN A,22
	HRRM B,OUTP	;^R => TURN ON PTP CHARACTER OUTPUT
	MOVEI B,CPOPJ
	CAIN A,24
	HRRM B,OUTP	;^T => TURN OFF PTP CHARACTER OUTPUT
	CAIN A,2
	HRRM B,OUTLPT	;^B => TURN OFF LPT CHARACTER OUTPUT (NOTE: NO LPT ROUTINES)
	MOVE B,[PUSH P,B]
	CAIN A,22
	MOVEM B,OUTPB	;^R AGAIN, TURN ON PTP BINARY OUTPUT
	MOVSI B,(POPJ P,)
	CAIN A,24
	MOVEM B,OUTPB	;^T AGAIN, TURN OFF PTP BINARY OUTPUT
	MOVSI B,(SKIPA)
	CAIN A,26
	MOVEM B,OUTU	;^V, TURN OFF DECTAPE CHARACTER OUTPUT
	CAIN A,26
	MOVEM B,OUTUB	;^V AGAIN, TURN OFF BINARY DECTAPE OUTPUT
	MOVEI B,PILPT
	CAIN A,5
	HRRM B,OUTLPT	;^E, TURN ON LPT CHARACTER OUTPUT (NOTE: NO LPT ROUTINES)
	MOVE B,[PUSHJ P,UWR"]
	CAIN A,27
	MOVEM B,OUTU	;^W, TURN ON DECTAPE CHARACTER OUTPUT
	MOVE B,[PUSHJ P,.UWR"]
	CAIN A,27
	MOVEM B,OUTUB	;^W AGAIN, TURN ON DECTAPE BINARY OUTPUT
	CAIN A,7
	JRST 10,GO2	;^G, QUIT (NOW, DISMISSING INTERRUPT)
	CAIN A,^A
	JRST TYPON	;^A, COPY CHARS READ AT RCH ONTO TTY
	CAIN A,^C
	JRST TYPOF	;^C, TURN OFF ALL COPY OUT (OF CHARS READ AT RCH)
	IFN DSPLY,[CAIN A,^D
	JRST TYOB4	;^D, COPY CHARS READ AT RCH ONTO THE 340
]
