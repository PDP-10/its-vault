	JRST FAE	;FILE WITH NEW NAME ALREADY EXISTS
REN1:	POP P,D
	MOVEM A,(D)
	MOVEM B,1(D)
	POPJ P,

WINIT:	MOVEI A,^T
	PUSHJ P,TYOB
	MOVEI A,^W
	PUSHJ P,TYOB
	MOVE A,B
	PUSHJ P,OPNWR"
	JSR ERR
	POPJ P,

FAE:	TYPR1 [ASCIZ /FILE ALREADY EXISTS
/]
	JRST GO2

TPFUL:	TYPR1 [ASCIZ /TAPE FULL
/]
	JRST GO2



CRR:	MOVEI A,15
	PUSHJ P,TYO
	MOVEI A,12
	JRST TYO

DELE:	PUSHJ P,FRD	;ED COMMAND, DELETE FILE
	JRST UDELE"

.FILE:	MOVNI A,1	;EF COMMAND
	SKIPL CONTRL"
	PUSHJ P,PPB	;PUNCH NEGATIVE EOF FOR RELOCATABLE FILES
	PUSHJ P,FRD
	MOVEI C,3
	SKIPGE CONTRL"
	MOVEI C,2
	PUSHJ P,.FILEB"
	JRST TPFUL
	POPJ P,

CFIL3:	0	;.FNAM1
CFIL4:	0	;.FNAM2
CPTR:	0
ARG:	0
ARG2:	0
NUM:	0
NUM1:	0
DOLS:	0
TEM:	0

PUNBO:
	LOC PUNBO+50
PUNBE:	0
RBUF:
	LOC RBUF+20
RBEND:	0
RDCHAR==<RBEND-RBUF>*5
LOSSAG==RDCHAR/4

LOIIN:	MOVE A,[OISYMS,,EISYMT"]	;ADD TO INITIAL SYMBOL TABLE
	BLT A,EISYMT+LOISYM-1
	MOVEI A,EISYMT+LOISYM
	HRRM A,EISYMP"	;DOCUMENT DAMAGE DONE
	POPJ P,

LOISW:	-1		;-1 => OISYMS NOT YET IN SYMTAB

OISYMS:	SQUOZE 4,.FNAM1	;SYMS (PSEUDOS) HANDLED BY AIO
	CFIL3,,INTSYM"
	SQUOZE 4,.FNAM2
	CFIL4,,INTSYM
	SQUOZE 4,.INSRT	;LIKE NULL
	Z.INSRT		;UH HUH
LOISYM==.-OISYMS	;LENGTH OF THIS TABLE

Z.INSRT:	PUSHJ P,RCH"
	CAIE A,^M
	JRST .-2
	AOS (P)
	POPJ P,

CONSTANTS
CBUF:	BLOCK 20
END GO
