; -*- MIDAS -*-
	title	print AAA string on tty
a=1
b=2
c=3

chttyo==1			;channel for output

start:				;open tty channel.
	.call [setz ? sixbit /open/
		[.uao+%tjsio,,chttyo] ? [sixbit/tty/] ((setz))]
	 .lose %lsfil
	move b,[440700,,[ascii /[0m[>33;28;25;26;31l[>52;27;32hP`.z~V`,z~[V\[48;;;48p[H[J/]]
	movei c,.length        /[0m[>33;28;25;26;31l[>52;27;32hP`.z~V`,z~[V\[48;;;48p[H[J/
	.call [ setz ? sixbit /siot/
		movei chttyo ? b ? setz c]
	 .lose %lsfil
	.call [setz ? sixbit /scpos/ ? 0 ? 0 ? ((setz))]
	.logout 1,
end start
