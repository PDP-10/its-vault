BEGIN "TEST"

PROCEDURE LOSERR(INTEGER RSP)
    BEGIN
    DEFINE !BREAK = " '45000000000" ;
    EXTERNAL INTEGER JOBSA ;

    IF "a"  RSP  "z" THEN RSP _ RSP - '40 ;
    CASE RSP OF
	BEGIN
	["X"] START!CODE !BREAK '16,'40000 END;
	["S"] START!CODE MOVE 1,JOBSA; JRST (1) END;
	["D"] START!CODE !BREAK '16,'3000000 END;
	END;
    END "LOSERR";

WHILE TRUE DO BEGIN OUTSTR("WELL?"); LOSERR(INCHWL) END;

END "TEST";
