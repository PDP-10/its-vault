title MIDAS level support for EDIT package
.fasl
.insrt sys:.fasl defs

.entry GET-9-BYTE SUBR 003
	push p,[FIX1]		   ;be sure to return a fixnum
	move t,(a)		   ;get the machine number for fixnum A
	ldb tt,bytab9(t)	   ;get the apropriate byte
	popj p,			   ;and return it

;; byte pointers, to get byte n of fixnum in B do LDB TT,BYTAB9(T)
bytab9:	331100,,(b)
	221100,,(b)
	111100,,(b)
	001100,,(b)

FASEND
