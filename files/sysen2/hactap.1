TITLE MTAPE
A=1
B=2
C=3
MTC=1
JNAM=4

START:	.SUSET [.RJNAME,,JNAM]
	MOVE A,[-NCMDS,,CMDDPT]
	.OPEN MTC,[(SIXBIT /MT0/)]
	JRST RET
L1:	CAMN JNAM,(A)
	JRST CMD
	AOBJP A,.+1
	AOBJN A,L1
	JRST RET

CMD:	MOVE C,1(A)
	MOVE B,[MTC,,C]
	.MTAPE B,
	JFCL
RET:	.CLOSE MTC,
	.VALUE [ASCIZ /:KILL /

CMDDPT:	SIXBIT /REWIND/
	1
	SIXBIT /REWNDD/
	2
	SIXBIT /SPFR/	; SPACE FORWARD RECORD
	6
	SIXBIT /SPFF/	; FORWARD FILE
	7
	SIXBIT /SPRR/	; REV REC
	-1,,6
	SIXBIT /SPRF/	; REV FILE
	-1,,7
	SIXBIT /EOT/
	10
NCMDS==.-CMDDPT

END START
